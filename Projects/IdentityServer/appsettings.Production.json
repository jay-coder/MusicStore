{
  "Clients": [
    {
      "ClientId": "portal-client",
      "ClientName": "MusicStore Portal Client",
      "ClientUri": "https://portal.jaycoder.net",
      "RequireConsent": false,
      "AllowedGrantTypes": "implicit",
      "AllowAccessTokensViaBrowser": true,
      "RedirectUris": "https://portal.jaycoder.net",
      "PostLogoutRedirectUris": "https://portal.jaycoder.net/loggedout",
      "AllowedCorsOrigins": "https://portal.jaycoder.net",
      "AllowedScopes": "openid;profile;email;musicstore-api",
      "AlwaysIncludeUserClaimsInIdToken": true,
      "IdentityTokenLifetime": 3600,
      "AccessTokenLifetime": 3600
    }
  ]
}
